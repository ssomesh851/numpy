import numpy as np
import time
a = np.array([(1,2,3,4),(3,4,5,6),(7,8,9,10)])
print(a.reshape(4,2))
start = time.time()
print(start)



import numpy as np
import time
a = np.array([(1,2,3,4),(3,4,5,6),(7,8,9,10)])
print(a[0:2,2])
start = time.time()
print(start)
print(a.sum(axis=1))
print(a.sum(axis=0))
print(a.min())
print(a.max())
print(np.sqrt(a))
print(np.std(a))

import numpy as np
