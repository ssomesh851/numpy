import numpy as np
import time
import sys 
s = range(1000)
print(sys.getsizeof(s)*len(s))
start = time.time()
print(start)
D = np.array(s)
print(D.size*D.itemsize)
start1 = time.time()
print(start1)


import numpy as np
import sys
import time
size = 1000
l1=range(5718)
l2=range(5718)
start = time.time()
A1 = np.arange(size)
A2 = np.arange(size)
result = [(x,y) for x,y in zip(l1,l2)]
start = time.time()
result = A1+A2
print(result)
print((time.time()-start)*1000)

